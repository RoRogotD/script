local http_request = syn and syn.request or request;
local hwidPaste - loadstring(game: HttpGet("https://pastebin.com/Gae2KhfA"))()
local body - http_request({Url = 'https://httpbin.org/get'; Method = 'GET'}).Body; 
local decoded game:GetService("HttpService"): JSONDecode(body)
local hwid = decoded.headers['Syn-Fingerprint']

for i, v in pairs(hwidPaste) do
    if v == hwid then 
        game.StarterGui:SetCore("SendNotification", {
                    Title = "Whitelisted";
                    Text = "Script has been successfully loaded!",
                    Icon = "http://www.roblox.com/asset/?id=8709610734",
                    Duration = 3
                })
    else
        game.StarterGui:SetCore("SendNotification", {
                    Title = "Not Whitelisted";
                    Text = "your hwid has been copied please message RoRo got D#7597 to fully buy!",
                    Icon = "http://www.roblox.com/asset/?id=8709610734",
                    Duration = 3
                })
    end
end
print(game:GetService("RbxAnalyticsService"):GetClientId())

game.StarterGui:SetCore("SendNotification", {
                    Title = "‚ùì";
                    Text = "GUI loaded!",
                    Icon = "http://www.roblox.com/asset/?id=8709610734",
